<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.github.simonpercic.example.aircycle"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:name=".App"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">

        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".activity.a01.ListenerActivity"
            android:label="@string/listener_activity" />

        <activity
            android:name=".activity.a02.BundleListenerActivity"
            android:label="@string/bundle_listener_activity" />

        <activity
            android:name=".activity.a03.PassListenerActivity"
            android:label="@string/pass_listener_activity" />

        <activity
            android:name=".activity.a04.LifecycleCallbacksActivity"
            android:label="@string/lifecycle_callbacks_activity" />

        <activity
            android:name=".activity.a05.NullListenerActivity"
            android:label="@string/null_listener_activity" />

        <activity
            android:name=".activity.a06.MultipleListenersActivity"
            android:label="@string/multiple_listeners_activity" />

        <activity
            android:name=".activity.a07.CustomListenerActivity"
            android:label="@string/custom_listener_activity" />

        <activity
            android:name=".activity.a08.BindConfigActivity"
            android:label="@string/bind_config_activity" />

        <activity
            android:name=".activity.a09.DefaultConfigActivity"
            android:label="@string/default_config_activity" />

    </application>

</manifest>
